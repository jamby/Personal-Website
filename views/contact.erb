    <label for="first_name">First Name *</label>
    <input type="text" name="first_name" value="" size="30" maxlength="50" />
    <br />
    <label for="last_name">Last Name *</label>
    <input type="text" name="last_name" value="" size="30" maxlength="50" />
    <br />
    <label for="email">Email Address *</label>
    <input type="text" name="email" value="" size="30" maxlength="80" />
    <br />
    <label for="comments">Comments *</label>
    <textarea type="comments" name="comments" value= "" maxlength="1000" cols="28" rows="6"></textarea>
