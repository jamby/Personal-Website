<!DOCTYPE html>
<html>
  <head>
    <title>Jared Hamby // Game Programmer</title>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
    <script src="/javascripts/jquery-ui-1.8.18.custom.min.js" type="text/javascript"></script>
    <script src="/javascripts/application.js" type="text/javascript"></script>
    <!--<link href="stylesheets/960.css" media="screen" rel="stylesheet" type="text/css">-->
    <link type="text/css" href="/stylesheets/ui-lightness/jquery-ui-1.8.18.custom.css" media="screen" rel="stylesheet" />
    <link type="text/css" href="/stylesheets/application.css" media="screen" rel="stylesheet" />
	<meta http-equiv="X-UA-Compatible" content="IE=EmulateIE7">
  </head>
  <body>
  <div class="maincontainer">
    <div class="left">
		<div class="vertdivider"></div>
      <div class="navigation">
        <%= erb(:navigation)%>
      </div>
    </div>
    <div class="right">
      <% if @alert %>
        <div class="flash ui-corner-all"><%= @alert %></div>
      <% end %>
      <div class="header">
        <%= erb(:header)%>
      </div>
      <div class="content">
        <%= yield %>
      </div>
      <div class="footer">
        <%= erb(:footer)%>
      </div>
    </div>
  </div>
  </body>
</html>